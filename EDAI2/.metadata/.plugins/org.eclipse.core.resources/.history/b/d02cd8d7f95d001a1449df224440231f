package org.eda2;

public class Contador extends Medidores{
	
	private double consumo;
	
	public Contador() { //Consumo mensual
		this.consumo = 0;
	}
	
	public double getConsumo() {
		return this.consumo;
	}
	
	public void setConsumo(double consumo) {
		this.consumo = consumo;
	}
	
	@Override
	public String toString() {
		return "" + this.consumo;
	}

}
